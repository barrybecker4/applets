<?xml version="1.0"?>
<!DOCTYPE hierarchy SYSTEM "hierarchy.dtd">
<hierarchy title="Katate dori technique hierarchy" imgpath="images/katate_dori/" date="10/19/2004" author="Barry Becker">
  <node id="katate_dori" label="katate dori">
      <node id="tenkan_grabbed" label="tenkan entrance (grabbed)">
          <node id="sumi_grabbed" label="sumi entrance (grabbed)">
              <node id="sumi_otoshi_basic" label="sumi otoshi (basic)"/>
              <node id="sumi_otoshi_elbow" label="sumi otoshi (elbow)"/>
              <node id="sumi_otoshi_inside_shoulder" label="sumi otoshi (inside shoulder)"/>
              <node id="sumi_otoshi_outside_shoulder" label="sumi otoshi (outside shoulder)"/>
              <node id="sumi_otoshi_head" label="sumi otoshi (head)"/>
              <node id="sumi_koshinage_two_hands_on_arm" label="koshinage (morote seoinage)">
                  <node id="koshinage_right_finish" label="koshinage finish"/>
              </node>
              <node id="sumi_koshinage_arm_around_waist" label="koshinage (arm around waist)">
                  <use ref="koshinage_right_finish"/>
              </node>
              <node id="omote_uchi_zempo_nage" label="omote uchi zempo nage">
                  <node id="zempo_nage_left_finish" label="zempo nage"/>
              </node>
              <node id="ikkyo_omote_entrance" label="ikkyo omote entrance">
                  <node id="ikkyo_omote" label="ikkyo omote">
                      <node id="ikkyo_pin" label="ikkyo pin"/>
                  </node>
                  <node id="nikkyo" label="nikkyo">
                      <node id="nikkyo_ura_finish" label="nikkyo ura finish">
                          <node id="nikkyo_pin" label="nikkyo pin"/>
                      </node>
                      <node id="nikkyo_omote_finish" label="nikkyo omote finish">
                          <use ref="nikkyo_pin"/>
                      </node>
                  </node>
                  <node id="sankyo_hand_change" label="sankyo hand change">
                      <node id="sankyo" label="sankyo">
                          <node id="sankyo_ura" label="sankyo ura">
                              <node id="sankyo_seated_pin" label="sankyo seated pin"/>
                          </node>
                          <node id="sankyo_omote" label="sankyo omote">
                              <node id="sankyo_standing_pin" label="sankyo standing pin"/>
                          </node>
                          <node id="sankyo_zempo_nage" label="sankyo zempo nage">
                              <use ref="zempo_nage_left_finish"/>
                          </node>
                          <node id="sankyo_bring_around" label="sankyo bring around behind">
                              <node id="aiki_otoshi_zempo_nage" label="aiki otoshi zempo nage (?)">
                                  <node id="zempo_nage_right_finish" label="zempo nage (right foot?)"/>
                              </node>
                              <node id="koshinage_on_upper_arm" label="koshinage (on upper arm?)">
                                  <use ref="koshinage_right_finish"/>
                              </node>
                              <node id="tai_otoshi" label="tai otoshi (over leg)"/>
                          </node>
                      </node>
                  </node>
                  <node id="ikkyo_uchi" label="ikkyo uchi">
                      <node id="direct_downward_throw" label="direct downward throw"/>
                      <use ref="shihonage_entrance"/>
                  </node>
              </node>
              <node id="yonkyo_entrance" label="yonkyo entrance">
                  <node id="yonkyo_ura" label="yonkyo ura">
                      <node id="yonkyo_pin_standing" label="yonkyo pin (standing)"/>
                  </node>
                  <node id="yonkyo_omote" label="yonkyo omote">
                      <use ref="yonkyo_pin_standing" />
                  </node>
              </node>
              <node id="ikkyo_ura_entrance" label="ikkyo ura entrance">
                  <use ref="ikkyo_omote"/>
                  <use ref="nikkyo"/>
                  <use ref="sankyo_hand_change"/>
                  <use ref="yonkyo_entrance"/>
                  <node id="iriminage_continuance" label="iriminage continuance">
                      <node id="iriminage_right_throw" label="iriminage throw"/>
                      <node id="irminage_full_entrance" label="iriminage full entrance throw"/>
                  </node>
              </node>
              <node id="gokyo_entrance" label="gokyo entrance">
                  <node id="gokyo_omote" label="gokyo omote">
                      <node id="gokyo_pin" label="gokyo pin"/>
                  </node>
                  <node id="gokyo_ura" label="gokyo ura">
                      <use ref="gokyo_pin"/>
                  </node>
              </node>
              <node id="uchi_mawari_grabbed" label="uchi mawari (grabbed)">
                  <use ref="zempo_nage_left_finish"/>
                  <node id="kaiten_nage_continuance" label="kaiten nage continuance">
                      <node id="kaiten_nage_omote" label="kaiten nage omote">
                          <node id="kaiten_nage_throw" label="kaiten nage throw"/>
                      </node>
                      <node id="kaiten_nage_ura" label="kaiten nage ura">
                          <use ref="kaiten_nage_throw"/>
                      </node>
                  </node>
                  <node id="arm_bar_continuance" label="arm bar continuance">
                      <node id="arm_bar" label="arm bar">
                          <node id="arm_bar_pin" label="arm bar pin"/>
                      </node>
                  </node>
                  <node id="koshi_nage_past_head" label="koshi nage (past head)">
                     <use ref="koshinage_right_finish"/>
                  </node>
              </node>

              <node id="soto_mawari_grabbed" label="soto mawari (grabbed)">
                  <use ref="zempo_nage_left_finish"/>
                  <use ref="kaiten_nage_continuance"/>
                  <use ref="arm_bar_continuance"/>
                  <node id="yonkyo_continuance" label="yonkyo continuance">
                      <use ref="yonkyo_omote"/>
                      <node id="sankyo_like_continuance_omote" label="sankyo-like continuance (omote)">
                          <use ref="sankyo_seated_pin"/>
                      </node>
                      <node id="sankyo_like_continuance_ura" label="sankyo-like continuance (ura)">
                          <use ref="sankyo_seated_pin"/>
                      </node>
                      <use ref="zempo_nage_left_finish"/>
                      <node id="uchi_yonkyo" label="uchi yonkyo">
                          <use ref="sumi_otoshi_elbow"/>
                          <use ref="sumi_otoshi_inside_shoulder"/>
                          <use ref="sumi_otoshi_outside_shoulder"/>
                          <use ref="sumi_otoshi_head"/>
                          <use ref="sumi_koshinage_two_hands_on_arm"/>
                          <use ref="sumi_koshinage_arm_around_waist"/>
                          <use ref="shihonage_entrance"/>
                      </node>
                  </node>
              </node>
              <node id="hiji_continuance" label="hiji continuance">
                  <node id="hijinage_throw" label="hijinage throw"/>
                  <use ref="sumi_otoshi_outside_shoulder"/>
                  <use ref="tai_otoshi"/>
              </node>
          </node>
          <node id="shihonage" label="shihonage">
              <node id="shihonage_regular_throw" label="shihonage throw (regular)"/>
              <node id="shihonage_high_throw" label="shihonage throw (high)"/>
          </node>
          <node id="hook_arm" label="hook_arm">
              <use ref="zempo_nage_left_finish"/>
          </node>
          <node id="continue_tenkan_180" label="continue temkan (180 degrees)">
              <use ref="kokyo_nage_grabbed"/>
              <use ref="zempo_nage_left_finish"/>
          </node>
      </node>
      <node id="kokyo_nage_grabbed" label="kokyo nage (grabbed)">
          <node id="kokyo_nage_throw" label="kokyo nage finish"/>
      </node>
      <use ref="sumi_grabbed"/>
      <node id="step_back_grabbed" label="step back">
          <use ref="ikkyo_omote_entrance"/>
          <use ref="ikkyo_ura_entrance"/>
          <use ref="yonkyo_entrance"/>
          <use ref="gokyo_entrance"/>
          <node id="step_forward_hand_to_head" label="step forward, raise hand">
              <use ref="shihonage"/>
              <use ref="zempo_nage_left_finish"/>
          </node>
      </node>
      <use ref="step_forward_hand_to_head"/>
      <node id="shihonage_entrance" label="shihonage entrance">
          <node id="shihonage" label="shihonage">
              <node id="shihonage_throw" label="shihonage throw (basic)"/>
              <node id="shihonage_throw_advanced" label="shihonage throw (advanced)"/>
          </node>
          <node id="udekimonage" label="udekimonage">
              <use ref="zempo_nage_left_finish"/>
          </node>
          <node id="kokyo_nage_grabbing" label="kokyo nage (grabbing)">
              <use ref="kokyo_nage_throw"/>
          </node>
          <node id="shiho_koshinage" label="shiho koshinage (hand at center)">
              <node id="koshinage_left_finish" label="kosinage finish"/>
          </node>
          <node id="kote_geishi_omote_continuance" label="kote geishi continuance (omote)">
              <node id="kote_geishi_throw" label="kote geishi"/>
          </node>
          <node id="kote_geishi_ura_continuance" label="kote geishi continuance (ura)">
              <use ref="kote_geishi_throw"/>
          </node>
          <node id="omote_iriminage" label="omote iriminage"/>
          <node id="koshinage_kata_guruma" label="kashinage (kata guruma)">
              <use ref="koshinage_right_finish"/>
          </node>
          <node id="koshinage_sukui_nage" label="koshinage (sukui nage)">
              <use ref="koshinage_right_finish"/>
          </node>
          <node id="arm_bar_over_shoulder" label="arm bar (over shoulder)">
              <node id="shiho_zempo_nage" label="shiho zempo nage">
                  <use ref="zempo_nage_right_finish"/>
              </node>
          </node>
      </node>
      <node id="irimi_entrance_break_grip" label="irimi entrance (break grip)">
          <use ref="iriminage_continuance"/>
      </node>
      <node id="reverse_kote_geishi" label="reverse kote geishi">
          <node id="reverse_kote_geishi_throw" label="reverse kote geishi throw"/>
      </node>
      <use ref="soto_mawari_grabbed"/>
      <node id="arc_hand_to_shoulder" label="arc hand to shoulder">
          <use ref="iriminage_right_throw"/>
      </node>
      <use ref="hiji_continuance"/>
      <node id="raise_hand" label="raise hand">
          <node id="turn_and_cut" label="turn and cut">
              <use ref="zempo_nage_left_finish"/>
          </node>
          <node id="iriminage_left_throw" label="iriminage throw"/>
      </node>
  </node>
</hierarchy>


