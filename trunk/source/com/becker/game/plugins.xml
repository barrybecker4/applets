<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE games SYSTEM "../source/com/becker/game/plugins.dtd">
<plugins date="10/19/2004" author="Barry Becker">
    <!-- whenever you add a new game to the framework, you need to add an extry here -->
    <!-- here are the 2 player games -->

    <game name="pente"
          msgKey="PENTE"
          serverPort="-1"
          panelClass="com.becker.game.twoplayer.pente.ui.PentePanel"
          controllerClass="com.becker.game.twoplayer.pente.PenteController" />
    <game name="checkers"
          msgKey="CHECKERS"
          serverPort="-1"
          panelClass="com.becker.game.twoplayer.checkers.ui.CheckersPanel"
          controllerClass="com.becker.game.twoplayer.checkers.CheckersController" />
    <game name="chess"
